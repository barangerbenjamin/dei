<p id="notice"><%= notice %></p>

<h1>Dei Fields</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Dei category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dei_fields.each do |dei_field| %>
      <tr>
        <td><%= dei_field.name %></td>
        <td><%= dei_field.dei_category_id %></td>
        <td><%= link_to 'Show', dei_field %></td>
        <td><%= link_to 'Edit', edit_dei_field_path(dei_field) %></td>
        <td><%= link_to 'Destroy', dei_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dei Field', new_dei_field_path %>
